{
  "name": "ger",
  "version": "1.0.0",
  "description": "gome error report js sdk",
  "main": "dist/ger.js",
  "directories": {
    "doc": "docs",
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "pretest": "jshint -c .jshintrc src",
    "test": "mocha --compilers js:babel-core/register -r jsdom-global/register",
    "devbuild": "rollup -c rollup.config.js",
    "build": "set NODE_ENV=production&&rollup -c rollup.config.js",
    "beautify": "node ./scripts/beatufyjs.js src test",
    "watch": "rollup -c  rollup.config.js -w",
    "start": "http-server -a 0.0.0.0 -p 8080 -s",
    "dev": "npm-run-all --parallel start watch"
  },
  "config": {
    "pre-git": {
      "pre-commit": [
        "npm run pretest",
        "npm run beautify",
        "git add ."
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gomeplusFED/GER.git"
  },
  "keywords": [
    "error",
    "report"
  ],
  "author": "xiaojue,sucon0302,zdongh2016",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gomeplusFED/GER/issues"
  },
  "homepage": "https://github.com/gomeplusFED/GER#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "http-server": "^0.9.0",
    "jsdom": "9.12.0",
    "jsdom-global": "2.1.1",
    "jshint": "^2.9.4",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "pre-git": "^3.13.0",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0"
  },
  "dependencies": {
    "mixin": "^0.2.0",
    "raven-js": "^3.10.0"
  }
}
